<div class="section">
    <div class="row">
        <div class="container">
          <div class="page-header">
              <h2>Sign In</h2>
              <%= link_to "New here? Create an account!", new_user_registration_path,
                class: "brand-blue" %>
          </div>
            <!--




            -->

            <div id='tabs'>
            <ul>
              <li><a href='#tabs-1'><span class='fa fa-globe'></span>Peace Corps Members</a></li>
              <li><a href='#tabs-2'><span class='fa fa-github'></span>Developers</a></li>
            </ul>
            <section class='tabs-content'>
              <article id='tabs-1'>
                <%= bootstrap_form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
                    <%= f.email_field :email, autofocus: true %>
                    <%= f.password_field :password, autocomplete: "off" %>
                    <% if devise_mapping.rememberable? -%>
                        <%= f.check_box :remember_me %>
                    <% end %>

                    <div class="actions">
                       <%= f.submit "Log in", class: "btn btn-success" %>
                    </div>
                    <br>
                    <%= link_to "Forgot your password?", new_password_path(resource_name),
                      class: "brand-blue" %>
                    <br><br>
                    <%= link_to "Didn't receive confirmation instructions?", new_confirmation_path(resource_name), class: "brand-blue"  %>
              </article>
              <article id='tabs-2'>
                <h3>Developers Sign In via Github OAuth</h3>
                  <%= link_to omniauth_authorize_path(resource_name, 'github') do %>
                    <div class="github-btn-dark">
                    	<span class="github-svg-container">
                    		<span class="github-icon-svg">
                          <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" viewBox="0 0 48 48">
                          <image width="48" height="48" xlink:href="data:img/png;base64,"/>
                          </svg>
                        </span>
                    	</span>
                    	<p class="btn-text">Sign in with GitHub</p>
                    </div>
                    <% end %>
                <% end %>
              </article>
            </section>
          </div>
        </div>
    </div>
</div>
<script type="text/javascript">
  $(function() {
      $( "#tabs" ).tabs();
  });
</script>
